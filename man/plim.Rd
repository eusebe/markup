\name{plim}
\alias{plim}
\title{format p values}
\usage{
  plim(p, digits = 2, decimal.mark = ".",
    drop0trailing = FALSE)
}
\arguments{
  \item{p}{p values}

  \item{digits}{number of digits}

  \item{decimal.mark}{see \code{?formatC}}

  \item{drop0trailing}{see \code{?formatC}}
}
\value{
  A character vector (formated p values)
}
\description{
  format p values
}
\author{
  David Hajage \email{dhajage@gmail.com}
}

